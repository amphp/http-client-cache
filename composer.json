{
    "name": "amphp/http-client-cache",
    "description": "An async HTTP cache for Amp's HTTP client.",
    "type": "library",
    "require": {
        "php": ">=8",
        "ext-json": "*",
        "ext-filter": "*",
        "ext-hash": "*",
        "amphp/amp": "^3-dev",
        "amphp/http": "^1.5",
        "amphp/http-client": "^5-dev",
        "amphp/cache": "^2-dev",
        "psr/log": "^1.1",
        "kelunik/string": "^1.0",
        "amphp/byte-stream": "^2-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "amphp/php-cs-fixer-config": "dev-master",
        "monolog/monolog": "^1.24 || ^2",
        "amphp/log": "^2-dev",
        "amphp/phpunit-util": "^2-dev",
        "amphp/file": "^3-dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Amp\\Http\\Client\\Cache\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "conflict": {
        "amphp/file": "<3 || >=4"
    },
    "scripts": {
        "test": "php vendor/bin/phpunit",
        "code-style": "php vendor/bin/php-cs-fixer fix"
    }
}
